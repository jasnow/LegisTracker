<h2>Bill Status</h2>
<% @events.each do |event| %>
	<p class="event"><%= event.bill.number %> <%= event.statusDate.to_s %> <%= event.statusCode %></p>
<% end %>

<h2>Votes</h2>
<% @votes.each do |vote| %>
	<p class="vote">
	   <%= vote.bill.number %> <%= vote.date.to_s %> <%= vote.description %><br />
	   Yea: <%= vote.yea %> Nay: <%= vote.nay %>
	</p>
<% end %>

<h2>House Press Releases</h2>
<% @house_rss[0..4].each do |item| %>
	<p><a class="rss house" href="<%= item.link %>"><%= item.title %></a></p>
<% end %>

<h2>Senate Press Releases</h2>
<% @senate_rss[0..4].each do |item| %>
	<p><a class="rss senate" href="<%= item.link %>"><%= item.title %></a></p>
<% end %>
	