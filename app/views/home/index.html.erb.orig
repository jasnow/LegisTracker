<div id="main" class="span-24 first last ">
	<div id="votes" class="span-5 push-1 first border prepend-top">
		
	<h2>Votes</h2>
	<p class="fwdbk" align="center"> 
	<% if @vote_date < Vote.last_date %>
		<%= link_to "Next >>", root_path( :vote_date => Date.parse( @vote_date ).tomorrow.to_s(:db), :status_date => @status_date ) %>
		
	<% end %> | 
	
	<%= link_to "<< Previous", root_path( :vote_date => Date.parse( @vote_date ).yesterday.to_s(:db), :status_date => @status_date ) %>
<br/>	Date: <%= @vote_date %>
	</p>
	
	<ul>
		<% @votes.each do |vote| %>
			<li class="vote">
			  <% unless vote.bill.nil? %>
          <%= link_to "#{vote.bill.number}", bill_path( vote.bill ) %>
				<% end %> 
        <%= vote.description %> &nbsp;<span class="tstamp">(<%= vote.date.strftime( '%l:%M %p' ) %>)</span><br/>
			<strong>   Yea: <%= vote.yea %> Nay: <%= vote.nay %> </strong>
			</li>
		<% end %>
	</ul>
		<p class="endy"> >>> end feed <<< </class> </p>
</div>

